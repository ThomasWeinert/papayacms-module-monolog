{
  "name": "papaya/cms-project",
  "description": "papaya CMS Project Skeleton",
  "license": "GPL-2.0",
  "homepage": "http://www.papaya-cms.com/",
  "extra" : {
    "document-root" : "htdocs/",
    "papaya" : {
      "template-directory": "templates/"
    }
  },
  "require": {
    "papaya/cms-core": "dev-master@dev",
    "papaya/module-default": "1.*",
    "papaya/module-standard": "1.*",
    "monolog/monolog": "^2.0@dev"
  },
  "require-dev": {
    "papaya/theme-dynamic": "1.*",
    "papaya/cms-administration": "dev-master@dev",
    "papaya/test-framework": "dev-master@dev",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Papaya\\Module\\Monolog\\" : "src/"
    }
  }
}
